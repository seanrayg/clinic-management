@model IEnumerable<clinic_management.Models.SupplyChanges>

@{
    ViewBag.Title = "ViewEditsSupply";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Supply Edits for @ViewBag.ItemName</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <table class="table" id="tableSupplyEdits">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.DateChange)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ChangeReason)
                    </th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
            {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.DateChange)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ChangeReason)
                        </td>
                    </tr>
                }
            </tbody>

        </table>
    </div>
    <div class="col-md-3"></div>
</div>

<script>
    $(document).ready(function () {
        $('#tableSupplyEdits').DataTable();
    });
</script>