@model IEnumerable<clinic_management.Models.PatientType>

@{
    ViewBag.Title = "Recover";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Recover @Html.ActionLink("Back", "Admin", "Deletes", null, new { @class = "btn btn-default pull-right col-md-1" })</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
        <table class="table" id="tablePatientTypes">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.TypeName)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
            {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.TypeName)
                        </td>
                        <td>
                            @Html.ActionLink("Recover", "Recover", new { id = item.TypeID })
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

<script>
    $(document).ready(function () {
        $('#tablePatientTypes').DataTable();
    });
</script>
